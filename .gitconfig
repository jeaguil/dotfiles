[user]
    name = Jesus Aguilera
    email = jeaguil@github.com
[github]
    user = jeaguil
[alias]
    ll = !. ~/.dotfiles/.githelpers && pretty_git_log
    la = !git ll --all
    p-re = !. ~/.dotfiles/.githelpers && pull_remote
    s-pr = !. ~/.dotfiles/.githelpers && switch_prefix
    su = !. ~/.dotfiles/.githelpers && set_upstream
    clb = !. ~/.dotfiles/.githelpers && clean_local_branches
    bvv = !. ~/.dotfiles/.githelpers && color_verbose_branch
[core]
    pager = less -F -X
    editor = vim
[pager]
    branch = false
    diff = false
    log = false
[push]
    default = current
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true